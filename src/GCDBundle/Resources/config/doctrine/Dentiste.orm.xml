<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GCDBundle\Entity\Dentiste" table="dentiste">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomDentiste" type="string" column="nom_dentiste" length="50" nullable="false"/>
    <field name="prenomDentiste" type="string" column="prenom_dentiste" length="50" nullable="false"/>
    <field name="emailDentiste" type="string" column="email_dentiste" length="50" nullable="false"/>
    <field name="password" type="string" column="password" length="50" nullable="false"/>
    <field name="adresseDentiste" type="string" column="adresse_dentiste" length="50" nullable="false"/>
    <field name="telDentiste" type="integer" column="tel_dentiste" nullable="false"/>
    <field name="adresseCabinet" type="text" column="adresse_cabinet" nullable="false"/>
    <field name="telCabinet" type="integer" column="tel_cabinet" nullable="false"/>
    <field name="sexe" type="string" column="sexe" length="10" nullable="false"/>
  </entity>
</doctrine-mapping>
